{
  "name": "wetube-clone",
  "version": "1.0.0",
  "description": "Cloning Youtube with Vanilla and NodeJS",
  "build": "cross-env NODE_ENV=production webpack --config build/webpack.config.js",
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore '.scss' --ignore 'static' ",
    "dev:assets": "cd src && WEBPACK_ENV=development webpack -w",
    "build:assets": "cd src && WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.js'",
    "copyAll": "cp -R src/static build && cp -R src/views build",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "prebuild": "rm -rf build",
    "tunnel": "lt --port 4000",
    "start": "PRODUCTION=true node build/init.js",
    "prestart": "npm run build"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "eslint": "^6.8.0",
    "nodemon": "^1.19.4"
  },
  "author": "Minkyu Kim",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.8.4",
    "@babel/runtime": "^7.9.2",
    "autoprefixer": "^9.7.4",
    "aws-sdk": "^2.643.0",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "brew": "0.0.8",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.4",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "get-blob-duration": "^1.1.0",
    "helmet": "^3.21.2",
    "mongodb": "^3.5.3",
    "mongoose": "^5.9.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "node-sass": "^4.13.1",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.4",
    "sass-loader": "^8.0.2",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "wepack-cli": "0.0.1-security"
  }
}
